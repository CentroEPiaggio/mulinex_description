<?xml version="1.0"?>

<robot xmlns:xacro="http://ros.org/wiki/xacro">

    <xacro:include filename="$(find mulinex_description)/urdf/links.xacro" />
    <xacro:include filename="$(find mulinex_description)/urdf/wheel.xacro" />
    <xacro:property name="PI" value="3.141592" />

    <xacro:macro name="omni_mod" params="prefix gazebo jnt_pref simplify_meshes">
        <!-- Module 1 -->
        <xacro:hip
            prefix="${prefix}"
            gazebo="${gazebo}"
            jnt_pref=""
            simplify_meshes="${simplify_meshes}"
        />
        <!-- wheel -->
        <xacro:wheel
            prefix="${prefix}"
            gazebo="${gazebo}"
        />
        <!-- Lower leg link -->
        
    </xacro:macro>

</robot>